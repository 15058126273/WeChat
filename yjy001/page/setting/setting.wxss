.setting{
    margin-top: 50rpx;
}
.setting_item{
    border-top: 1px solid #D2D2D2;
    border-bottom: 1px solid #D2D2D2;
    width: 100%;
    height: 40px;
    line-height: 40px;
    display: table;
    text-align: center;
}
.setting_text{
    font-family: Microsoft YaHei;
    float: left;
    margin-left: 30rpx;
    font-size: 14px;
    color: grey;
}
.item_other{
    border-top: 0px;
}
.setting_button{
    font-family: Microsoft YaHei;
    font-size: 14px;
    color: grey;
}
.setting_enter{
    font-family: Microsoft YaHei;
    float: right;
    margin-right: 30rpx;
    font-size: 14px;
    color: grey;
}
.setting_switch{
    height: 32px;
    float: right;
    margin-right: 30rpx;
    margin-top: 4px;
}