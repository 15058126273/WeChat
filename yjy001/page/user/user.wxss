.data_head{
    width: 600rpx;
    height: 100px;
    border-bottom: 2rpx solid grey;
    margin: 20rpx auto 0px auto;
}
.data_image{
    width: 60px;
    height: 60px;
    border-radius: 30px;
    margin: 50rpx auto 0px auto;
    display: block
}
.data_sign{
    width: 500rpx;
    margin: 20rpx auto 0px auto;
    display: block;
    font-size: 25rpx;
    color: grey;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    text-align: center;
}